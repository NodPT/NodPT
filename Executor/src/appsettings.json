{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Executor": {
    "RedisConnection": "nodpt-redis:6379",
    "MaxManager": 0,
    "MaxInspector": 0,
    "MaxAgent": 0,
    "MaxTotal": 0,
