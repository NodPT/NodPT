{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Cors": {
    "AllowedOrigins": [
      "https://yourdomain.com",
      "https://www.yourdomain.com"
    ]
  },
  "Redis": {
    "ConnectionString": "redis:6379",
    "StreamListener": {
      "MessageBatchSize": 10,
      "PollingDelayMs": 100,
      "ErrorRetryDelayMs": 1000
    }
  }
}
